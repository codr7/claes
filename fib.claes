(^ fib-rec [n]
  (if (< n 2) n (+ (fib-rec (decr n)) (fib-rec (decr n)))))

(^ fib-tail [n a b]
  (if (> n 1) (fib-tail (decr n) b (+ a b)) (if (z? n) a b)))

(^ fib-lookup [n table]
  (say n table)
  (or ((table) n) 
       (let [result (if (< n 2) n (+ (fib-lookup (- n 1) table)
                                     (fib-lookup (- n 2) table)))] 
         ((table) n result))))

(fib-lookup 10 &{})